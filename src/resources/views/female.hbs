<div class="Container">
    <h2 class="title text-left mona_title">ĐỒNG HỒ NỮ <hr></h2>
		<label for="customRange1" class="form-label">Giá sản phẩm</label>
	<input type="range" class="form-range" id="customRange1">
	<div class="form-check form-check-inline">
  <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="option1">
  <label class="form-check-label" for="inlineCheckbox1">CASIO</label>
</div>
<div class="form-check form-check-inline">
  <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2">
  <label class="form-check-label" for="inlineCheckbox2">CITIZEN</label>
</div>
<div class="form-check form-check-inline">
  <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2">
  <label class="form-check-label" for="inlineCheckbox2">TISSOT</label>
</div>
<div class="form-check form-check-inline">
  <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2">
  <label class="form-check-label" for="inlineCheckbox2">MOVADO</label>
</div>
<button>Tìm kiếm</button>
	<div class="mt-4 ">
		<div class="row">
			{{#each femaleList}}
			<div class="col-3 mt-4 mb-4 height">
				<div class="card card-item border border-white border-0">
					<a href="/courses/{{this.name_content}}">
						<img class="card-img-top" src="{{this.img}}" alt="Card image cap">
					</a>
					<div class="card-body">
						<a href="/courses/{{this.name_content}}" class="text-dark">
							<h5 class="card-title break-word text-center">{{this.name_content}}</h5>
							<p class="card-text text-center">{{this.total}}$</p>
						</a>
					</div>
				</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>